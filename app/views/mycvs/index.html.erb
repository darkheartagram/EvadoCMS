<h1>Listing mycvs</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mycvs.each do |mycv| %>
  <tr>
    <td><%= mycv.content %></td>
    <td><%= mycv.user_id %></td>
    <td><%= mycv.type_id %></td>
    <td><%= link_to 'Show', mycv %></td>
    <td><%= link_to 'Edit', edit_mycv_path(mycv) %></td>
    <td><%= link_to 'Destroy', mycv, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mycv', new_mycv_path %>
